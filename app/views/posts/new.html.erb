
<%= form_with model:@post, local:true do |f| %>

<h4>投稿画像</h4>
<%= f.attachment_field :image %>

<h4>タイトル</h4>
<%= f.text_field :title %>

<h4>都道府県</h4>
<%= f.text_field :prefectures %>

<h4>おすすめ季節</h4>
<%= f.text_field :season %>

<h4>紹介文</h4>
<%= f.text_area :introduction %>

<h4>ひとこと！</h4>
<%= f.text_field :word %>

<%= f.submit '投稿' %>

<% end %>

